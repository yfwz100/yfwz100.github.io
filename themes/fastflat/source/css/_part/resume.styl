.resume.wrap
  .page
    padding-left 1.5em

    h1
      font-size 1.2em
      position relative

      &:not(:first-child)
        padding-top 1em

      &:before
        content "#"
        color #0cf
        position absolute
        left -1em

    table
      width 100%

      th
        text-align right
